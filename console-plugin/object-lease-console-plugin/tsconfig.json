{
  "compilerOptions": {
    "target": "ES2019",
    "module": "ESNext",
    "lib": ["ES2020", "DOM"],
    "jsx": "react",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "types": ["node", "react", "react-dom"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {}
  },
  "include": ["src/**/*"]
}
